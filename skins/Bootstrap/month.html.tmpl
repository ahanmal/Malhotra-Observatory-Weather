    <div class="container-fixed"> <!--Main page container -->  
      <div class="row">
        <div class="span4">
        
          <!-- <ul class="nav nav-pills">
            <li class="active"><a href="#">Now</a></li>
          </ul> -->          
          <ul class="thumbnails">
            <li class="span2">
              <img src="outTempGauge.png" alt="">
              <h5 align="center">Outside temperature</h5>
            </li>
            <li class="span2">
              <img src="barometerGauge.png" alt="">
              <h5 align="center">Barometric pressure</h5>
            </li>
            <li class="span2">
              <img src="windDirGauge.png" alt="">
              <h5 align="center">Wind direction</h5>
            </li>
            <li class="span2">
                <img src="outHumidityGauge.png" alt="">
              <h5 align="center">Outside humidity</h5>
            </li>

            <li class="span2">
              <img src="windSpeedGauge.png" alt="">
              <h5 align="center">Wind speed</h5>
            </li>
            <li class="span2">
                <img src="windGustGauge.png" alt="">
              <h5 align="center">Gust speed</h5>
            </li>
          </ul>    
          
          <!-- <div class="well"> -->
            <table class="table table-hover">
              <tbody>
	              <tr>
	                <td>Latitude:</td>
	                <td>$station.latitude[0]&deg; $station.latitude[1]' $station.latitude[2]</td>
	              </tr>
	              <tr>
	                <td>Longitude:</td>
	                <td>$station.longitude[0]&deg; $station.longitude[1]' $station.longitude[2]</td>
	              </tr>
	              <tr>
	                <td>Altitude:</td>
	                <td>$station.altitude</td>
	              </tr>
	              <tr>
	                <td>Station uptime:</td>
	                <td>$station.uptime</td>
	              </tr>
	              <tr>
	                <td>Server uptime:</td>
	                <td>$station.os_uptime</td>
	              </tr>
	            </tbody>
            </table>

<!--           </div> -->

          
        </div><!--/span-->
        <div class="span8">
          <ul class="nav nav-pills">
            <li><a href="index.html">Last 24 hours</a></li>
            <li><a href="week.html">Last week</a></li>
            <li class="active"><a href="#">Last month</a></li>
            <li><a href="year.html">Last year</a></li>
          </ul>

          <div class="row">
          
            <div class="span4">
              <a href="big_images/monthtempchill-Bootstrap.png" rel="lightbox" title="Temperature and Wind Chill">
                <img src="monthtempchill-Bootstrap.png" alt="">
              </a>
            </div>
            
            <div class="span4">
              <a href="big_images/monthbarometer-Bootstrap.png" rel="lightbox" title="Barometric Pressure"><img src="monthbarometer-Bootstrap.png" alt=""></a>              
            </div>
            
          </div><!--/row-->
	          
          <br>
          <div class="row">
            <div class="span4">
              <a href="big_images/monthrain-Bootstrap.png" rel="lightbox" title="Rainfall"><img src="monthrain-Bootstrap.png" alt=""></a>              
            </div>
            
            <div class="span4">
              <a href="big_images/monthhumidity-Bootstrap.png" rel="lightbox" title="Outside Humidity"><img src="monthhumidity-Bootstrap.png" alt=""></a>
            </div>
            
          </div><!--/row-->
          <br>
          <div class="row">
            <div class="span4">
              <a href="big_images/monthwind-Bootstrap.png" rel="lightbox" title="Wind Speed"><img src="monthwind-Bootstrap.png" alt=""></a>
            </div>
				
            <div class="span4">
              <a href="big_images/monthwinddir-Bootstrap.png" rel="lightbox" title="Wind Direction"><img src="monthwinddir-Bootstrap.png" alt=""></a>     
            </div>
	         
          </div><!--/row-->
        </div><!--/span-->
      </div><!--/row-->

